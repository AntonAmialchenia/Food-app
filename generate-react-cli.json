{
  "usesTypeScript": true,
  "usesCssModule": false,
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/default/TemplateName.tsx",
        "index": "templates/default/index.ts"
      },
      "path": "src/components",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withIndex": true
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/TemplateName.tsx",
        "index": "templates/page/index.ts"
      },
      "path": "src/pages",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withIndex": true
    }
  },
  "usesStyledComponents": false,
  "cssPreprocessor": "css"
}
